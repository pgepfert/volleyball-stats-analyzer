CREATE TABLE "Formation" (
	"id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	"gameID"	INTEGER,
	"actionNr"	INTEGER,
	"h1"	INTEGER,
	"h2"	INTEGER,
	"h3"	INTEGER,
	"h4"	INTEGER,
	"h5"	INTEGER,
	"h6"	INTEGER,
	"v1"	INTEGER,
	"v2"	INTEGER,
	"v3"	INTEGER,
	"v4"	INTEGER,
	"v5"	INTEGER,
	"v6"	INTEGER,
	FOREIGN KEY("gameID") REFERENCES "Game"("id") ON DELETE CASCADE
)