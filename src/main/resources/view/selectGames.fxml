<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.CheckListView?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="700.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="controller.SelectGamesController">

    <Label fx:id="selectGamesLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="7.0" layoutY="14.0"
